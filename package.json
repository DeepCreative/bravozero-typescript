{
  "name": "@bravozero/sdk",
  "version": "1.0.0",
  "description": "Official TypeScript/JavaScript SDK for Bravo Zero Breaking the Limits APIs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "bravo-zero",
    "ai",
    "governance",
    "memory",
    "sdk",
    "typescript"
  ],
  "author": "Bravo Zero <sdk@bravozero.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bravozero/bravozero-typescript.git"
  },
  "homepage": "https://docs.bravozero.ai/sdk/typescript",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@noble/ed25519": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
